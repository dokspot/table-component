{
  "name": "@dokspot/table-component",
  "version": "0.1.13",
  "description": "React table component",
  "author": "dokspot",
  "repository": "https://github.com/dokspot/table-component.git",
  "bugs": {
    "url": "https://github.com/dokspot/table-component/issues"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@styled-icons/material": "^10.28.0",
    "bootstrap": "^4.6.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "react-table": "^7.6.3",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "release": "auto shipit --base-branch=master",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/node-logger": "^6.1.15",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.15",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^12.1.10",
    "auto": "^10.24.3",
    "babel-plugin-auto-import": "^1.1.0",
    "cross-env": "^7.0.3",
    "react-test-renderer": "^17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/dokspot/table-component#readme",
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ],
    "plugins": [
      [
        "auto-import",
        {
          "declarations": [
            {
              "default": "React",
              "path": "react"
            }
          ]
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "main": "dist/index.js"
}
